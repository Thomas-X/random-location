{
  "name": "random-location",
  "version": "1.0.12",
  "description": "Random coordinates within a circle given a center point and radius",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:babel": "babel --ignore '**/*.test.js' src -d dist --verbose",
    "build:webpack": "webpack --display-modules ./src -o dist/randomLocation.umd.js",
    "validate": "npm run cover && npm run check-coverage",
    "setup": "npm install && npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rmrs/random-location.git"
  },
  "keywords": ["random", "location", "coordinates", "radius", "central", "center", "point"],
  "files": ["dist/", "LICENSE", "README.md", "package.json"],
  "author": "Sagi Kedmi (https://sagi.io) <git@sagi.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rmrs/random-location/issues"
  },
  "homepage": "https://github.com/rmrs/random-location",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "3.0.0",
    "seedrandom": "^3.0.5",
    "webpack": "4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
